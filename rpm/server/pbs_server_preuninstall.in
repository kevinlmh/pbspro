#!/bin/sh
ldconfig @CPACK_PACKAGING_INSTALL_PREFIX@/@CMAKE_INSTALL_LIBDIR@

if [ "$1" != "1" ]; then
	# This is an uninstall, not an upgrade.
    @CPACK_PACKAGING_INSTALL_PREFIX@/@CMAKE_INSTALL_LIBEXECDIR@/pbs_preuninstall server \
	    @PBS_VERSION@ @CPACK_PACKAGING_INSTALL_PREFIX@ @HAVE_SYSTEMD@
fi